{
  "name": "childrens-health",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "homepage": "https://childrens.weareasterisk.com",
  "author": {
    "name": "Muhammad Khatri",
    "email": "muhammad@weareasterisk.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weareasterisk/childrens-health.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development run-p build:css:watch start:code",
    "start:code": "FORCE_COLOR=true react-scripts start | cat",
    "build": "cross-env NODE_ENV=production run-s build:css build:code",
    "build:code": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:css": "postcss src/tailwind.scss -o src/generated.scss",
    "build:css:watch": "postcss src/tailwind.scss -o src/generated.scss -w",
    "clean": "rimraf build node_modules",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-facebook-pixel": "^0.1.3",
    "react-media": "^1.10.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^9.7.4",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "glob-all": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.5.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "purgecss-webpack-plugin": "^2.0.6",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.2.0",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
